{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "frontendEcommerceVisiteur": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
                "jquery",
                "bootstrap-notify"
            ],
            "outputPath": "dist/frontendEcommerceVisiteur",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/inclus/bootstrap.min.css",
              "src/assets/css/font-awesome.min.css",

              "src/assets/css/style.css",
              "src/assets/css/responsive.css",
              "src/assets/css/bootstrap.min.css"
            ],
            "scripts": [
              "src/assets/vendors/js/vendor.bundle.base.js",
              "src/assets/js/off-canvas.js",
              "src/assets/js/hoverable-collapse.js",
              "src/assets/js/misc.js",
              "src/assets/js/settings.js",
              "src/assets/js/todolist.js",



              "src/assets/js/inclus/jquery.min.js",
              "src/assets/js/inclus/jquery.nivo.slider.js",
              "src/assets/js/inclus/popper.min.js",
              "src/assets/js/inclus/notify.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "frontendEcommerceVisiteur:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "frontendEcommerceVisiteur:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "frontendEcommerceVisiteur:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/inclus/bootstrap.min.css",
              "src/assets/css/font-awesome.min.css"
            ],
            "scripts": [
              "src/assets/vendors/js/vendor.bundle.base.js",
              "src/assets/js/off-canvas.js",
              "src/assets/js/hoverable-collapse.js",
              "src/assets/js/misc.js",
              "src/assets/js/settings.js",
              "src/assets/js/todolist.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "frontendEcommerceVisiteur:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "frontendEcommerceVisiteur:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "frontendEcommerceVisiteur"
}
